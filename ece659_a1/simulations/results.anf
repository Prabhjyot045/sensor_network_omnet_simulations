        </chart>
        <chart createdWith="OMNeT++ 6.3.0.251110-bb3d6a4731" icon="icons/full/obj/scatterchart.png" id="554422244" name="Throughput_vs_node_speed" resultTypes="scalar" template="scatterchart_itervars_mpl" type="MATPLOTLIB">
            <script><![CDATA[from omnetpp.scave import results, chart, utils, ideplot
import pandas as pd
import numpy as np
import matplotlib.pyplot as plt

# get chart properties
props = chart.get_properties()
utils.preconfigure_plot(props)

# Query throughput from sink apps - using the vector mean
try:
    df_tp = results.get_scalars(
        'name=~"throughput:vector:mean"',
        include_fields=True, include_attrs=True, include_runattrs=True, include_itervars=True
    )
except results.ResultQueryError as e:
    raise chart.ChartScriptError("Error querying throughput data: " + str(e))

if df_tp.empty:
    # Fallback: calculate from received packets
    try:
        df_recv = results.get_scalars(
            'name=~"packetReceived:sum(packetBytes)"',
            include_fields=True, include_attrs=True, include_runattrs=True, include_itervars=True
        )
        if df_recv.empty:
            raise chart.ChartScriptError("No throughput or packet data found.")
        
        # Calculate throughput: total bytes / simulation time
        SIMULATION_TIME = 898  # seconds
        df_recv['speed'] = df_recv['speed'].astype(int)
        
        def extract_mobility(configname):
            if 'RandomWaypoint' in configname:
                return 'Random Waypoint'
            elif 'RandomWalk' in configname:
                return 'Random Walk'
            return 'Unknown'
        
        df_recv['mobility'] = df_recv['configname'].apply(extract_mobility)
        
        # Sum bytes per run and convert to kbps
        tp_per_run = df_recv.groupby(['runID', 'configname', 'mobility', 'speed'])['value'].sum().reset_index()
        tp_per_run['throughput_kbps'] = (tp_per_run['value'] * 8) / (SIMULATION_TIME * 1000)
        
    except results.ResultQueryError as e:
        raise chart.ChartScriptError("Error querying packet data: " + str(e))
else:
    # Use throughput:vector:mean directly (value is in bps)
    df_tp['speed'] = df_tp['speed'].astype(int)
    
    def extract_mobility(configname):
        if 'RandomWaypoint' in configname:
            return 'Random Waypoint'
        elif 'RandomWalk' in configname:
            return 'Random Walk'
        return 'Unknown'
    
    df_tp['mobility'] = df_tp['configname'].apply(extract_mobility)
    
    # Convert bps to kbps
    df_tp['throughput_kbps'] = df_tp['value'] / 1000
    
    # Calculate mean throughput per run (average across all sink nodes)
    tp_per_run = df_tp.groupby(['runID', 'configname', 'mobility', 'speed'])['throughput_kbps'].mean().reset_index()

# Calculate mean and std for each speed and mobility model (across repetitions)
tp_stats = tp_per_run.groupby(['mobility', 'speed'])['throughput_kbps'].agg(['mean', 'std']).reset_index()
tp_stats.columns = ['mobility', 'speed', 'throughput_mean', 'throughput_std']
tp_stats = tp_stats.sort_values(['mobility', 'speed'])

# Plot using current axes
ax = plt.gca()

markers = {'Random Waypoint': 'o', 'Random Walk': 's'}
colors = {'Random Waypoint': 'tab:orange', 'Random Walk': 'tab:blue'}

for mobility in sorted(tp_stats['mobility'].unique()):
    data = tp_stats[tp_stats['mobility'] == mobility].sort_values('speed')
    ax.errorbar(data['speed'], data['throughput_mean'], yerr=data['throughput_std'], 
                marker=markers.get(mobility, '^'), capsize=5, label=mobility, 
                linewidth=2, markersize=8, color=colors.get(mobility, None))

ax.set_xlabel('Node Speed (m/s)', fontsize=12)
ax.set_ylabel('Throughput (kbps)', fontsize=12)
ax.set_title('Network Throughput vs Node Speed', fontsize=14)
ax.legend(loc='best', fontsize=10)
ax.grid(True, linestyle='--', alpha=0.7)
ax.set_xlim(0, 22)
ax.set_xticks([1, 5, 10, 15, 20])

plt.tight_layout()

utils.postconfigure_plot(props)
utils.export_image_if_needed(props)
utils.export_data_if_needed(tp_per_run, props)]]></script>
