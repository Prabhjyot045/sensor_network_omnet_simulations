<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<analysis version="2">
    <inputs>
        <input pattern="results/*.sca"/>
        <input pattern="results/*.vec"/>
    </inputs>
    <charts>
        <chart createdWith="OMNeT++ 6.3.0.251110-bb3d6a4731" icon="icons/full/obj/scatterchart.png" id="2075930125" name="PDR_vs_node_speed" resultTypes="scalar" template="scatterchart_itervars_mpl" type="MATPLOTLIB">
            <script><![CDATA[from omnetpp.scave import results, chart, utils, ideplot
import pandas as pd
import numpy as np
import matplotlib.pyplot as plt

# get chart properties
props = chart.get_properties()
utils.preconfigure_plot(props)

# Query sent packets from source apps (hosts 1-24)
try:
    df_sent = results.get_scalars(
        'module=~"*.host[*].app[0]" AND name=~"packetSent:count"',
        include_fields=True, include_attrs=True, include_runattrs=True, include_itervars=True
    )
except results.ResultQueryError as e:
    raise chart.ChartScriptError("Error querying sent packets: " + str(e))

# Query received packets from sink (host[0])
try:
    df_recv = results.get_scalars(
        'module=~"*.host[0].app[0]" AND name=~"packetReceived:count"',
        include_fields=True, include_attrs=True, include_runattrs=True, include_itervars=True
    )
except results.ResultQueryError as e:
    raise chart.ChartScriptError("Error querying received packets: " + str(e))

if df_sent.empty or df_recv.empty:
    raise chart.ChartScriptError("The result filter returned no data.")

# Convert speed to integer
df_sent['speed'] = df_sent['speed'].astype(int)
df_recv['speed'] = df_recv['speed'].astype(int)

# Aggregate sent packets per run (sum from all sources)
sent_per_run = df_sent.groupby(['runID', 'configname', 'speed'])['value'].sum().reset_index()
sent_per_run.columns = ['runID', 'configname', 'speed', 'sent']

# Received packets per run (from sink)
recv_per_run = df_recv.groupby(['runID', 'configname', 'speed'])['value'].sum().reset_index()
recv_per_run.columns = ['runID', 'configname', 'speed', 'received']

# Merge and calculate PDR
merged = pd.merge(sent_per_run, recv_per_run, on=['runID', 'configname', 'speed'], how='left')
merged['received'] = merged['received'].fillna(0)
merged['PDR'] = (merged['received'] / merged['sent']) * 100

# Extract mobility model from configname
def extract_mobility(configname):
    if 'RandomWaypoint' in configname:
        return 'Random Waypoint'
    elif 'RandomWalk' in configname:
        return 'Random Walk'
    return 'Unknown'

merged['mobility'] = merged['configname'].apply(extract_mobility)

# Calculate mean and std for each speed and mobility model
pdr_stats = merged.groupby(['mobility', 'speed'])['PDR'].agg(['mean', 'std']).reset_index()
pdr_stats.columns = ['mobility', 'speed', 'pdr_mean', 'pdr_std']
pdr_stats = pdr_stats.sort_values(['mobility', 'speed'])

# Plot using current axes
ax = plt.gca()

markers = {'Random Waypoint': 'o', 'Random Walk': 's'}
colors = {'Random Waypoint': 'tab:orange', 'Random Walk': 'tab:blue'}

for mobility in sorted(pdr_stats['mobility'].unique()):
    data = pdr_stats[pdr_stats['mobility'] == mobility].sort_values('speed')
    ax.errorbar(data['speed'], data['pdr_mean'], yerr=data['pdr_std'], 
                marker=markers.get(mobility, '^'), capsize=5, label=mobility, 
                linewidth=2, markersize=8, color=colors.get(mobility, None))

ax.set_xlabel('Node Speed (m/s)', fontsize=12)
ax.set_ylabel('Packet Delivery Ratio (%)', fontsize=12)
ax.set_title('Packet Delivery Ratio vs Node Speed', fontsize=14)
ax.legend(loc='best', fontsize=10)
ax.grid(True, linestyle='--', alpha=0.7)
ax.set_xlim(0, 22)
ax.set_ylim(0, 105)
ax.set_xticks([1, 5, 10, 15, 20])

plt.tight_layout()

utils.postconfigure_plot(props)
utils.export_image_if_needed(props)
utils.export_data_if_needed(merged, props)]]></script>
