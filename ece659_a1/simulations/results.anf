        <chart createdWith="OMNeT++ 6.3.0.251110-bb3d6a4731" icon="icons/full/obj/scatterchart.png" id="1360246833" name="Diagnostic" resultTypes="scalar" template="scatterchart_itervars_mpl" type="MATPLOTLIB">
            <script><![CDATA[from omnetpp.scave import results, chart, utils
import pandas as pd

# get chart properties
props = chart.get_properties()

print("=" * 80)
print("DIAGNOSTIC SCRIPT - Analyzing Available Data (Targeted Queries)")
print("=" * 80)

# ============================================================
# 1. Query ONLY packet sent from apps (small subset)
# ============================================================
print("\n" + "=" * 80)
print("1. PACKET SENT SCALARS FROM APPS")
print("=" * 80)

try:
    df_sent = results.get_scalars(
        'module=~"*.host[*].app[*]" AND name=~"*sent*"',
        include_fields=True, 
        include_attrs=True, 
        include_runattrs=True, 
        include_itervars=True
    )
    print(f"Records found: {len(df_sent)}")
    print(f"\nColumns: {df_sent.columns.tolist()}")
    if not df_sent.empty:
        print(f"\nUnique 'name' values: {df_sent['name'].unique().tolist()}")
        print(f"\nSample row:")
        for col, val in df_sent.iloc[0].items():
            print(f"  {col}: {val}")
except Exception as e:
    print(f"Error: {e}")

# ============================================================
# 2. Query ONLY packet received from apps
# ============================================================
print("\n" + "=" * 80)
print("2. PACKET RECEIVED SCALARS FROM APPS")
print("=" * 80)

try:
    df_recv = results.get_scalars(
        'module=~"*.host[*].app[*]" AND name=~"*received*"',
        include_fields=True, 
        include_attrs=True, 
        include_runattrs=True, 
        include_itervars=True
    )
    print(f"Records found: {len(df_recv)}")
    if not df_recv.empty:
        print(f"\nUnique 'name' values: {df_recv['name'].unique().tolist()}")
except Exception as e:
    print(f"Error: {e}")

# ============================================================
# 3. Query delay-related from apps
# ============================================================
print("\n" + "=" * 80)
print("3. DELAY SCALARS FROM APPS")
print("=" * 80)

try:
    df_delay = results.get_scalars(
        'module=~"*.host[*].app[*]" AND name=~"*[Dd]elay*"',
        include_fields=True, 
        include_attrs=True, 
        include_runattrs=True, 
        include_itervars=True
    )
    print(f"Records found: {len(df_delay)}")
    if not df_delay.empty:
        print(f"\nUnique 'name' values: {df_delay['name'].unique().tolist()}")
    else:
        print("No delay scalars found in apps")
except Exception as e:
    print(f"Error: {e}")

# ============================================================
# 4. Query ALL scalars from apps (just names, limited)
# ============================================================
print("\n" + "=" * 80)
print("4. ALL SCALAR NAMES FROM APPS (first run only)")
print("=" * 80)

try:
    df_apps = results.get_scalars(
        'module=~"*.host[0].app[*]"',
        include_fields=True, 
        include_attrs=True, 
        include_runattrs=True, 
        include_itervars=True
    )
    print(f"Records found: {len(df_apps)}")
    if not df_apps.empty:
        print(f"\nUnique 'name' values:")
        for name in sorted(df_apps['name'].unique()):
            print(f"  - {name}")
except Exception as e:
    print(f"Error: {e}")

# ============================================================
# 5. Check run attributes for ONE specific run
# ============================================================
print("\n" + "=" * 80)
print("5. RUN INFO FROM FIRST MATCHING SCALAR")
print("=" * 80)

try:
    df_sample = results.get_scalars(
        'module=~"*.host[0].app[0]" AND name=~"*"',
        include_fields=True, 
        include_attrs=True, 
        include_runattrs=True, 
        include_itervars=True
    )
    if not df_sample.empty:
        print(f"Total records: {len(df_sample)}")
        
        # Show all columns
        print(f"\nAll columns:")
        for col in sorted(df_sample.columns):
            print(f"  - {col}")
        
        # Show unique confignames
        if 'configname' in df_sample.columns:
            print(f"\nUnique confignames: {df_sample['configname'].unique().tolist()}")
        
        # Show unique runIDs (first 5)
        if 'runID' in df_sample.columns:
            print(f"\nSample runIDs: {df_sample['runID'].unique()[:5].tolist()}")
        
        # Look for speed in any column
        print(f"\nLooking for 'speed' in column names:")
        speed_cols = [col for col in df_sample.columns if 'speed' in col.lower()]
        if speed_cols:
            for col in speed_cols:
                print(f"  {col}: {df_sample[col].unique()[:10].tolist()}")
        else:
            print("  No column with 'speed' found directly")
            print("  Checking all 'itervar' columns:")
            itervar_cols = [col for col in df_sample.columns if 'itervar' in col.lower()]
            for col in itervar_cols:
                print(f"    {col}: {df_sample[col].unique()[:5].tolist()}")
except Exception as e:
    print(f"Error: {e}")

# ============================================================
# 6. Try to get end-to-end delay with different patterns
# ============================================================
print("\n" + "=" * 80)
print("6. SEARCHING FOR END-TO-END DELAY (various patterns)")
print("=" * 80)

patterns = [
    'name=~"*endToEnd*"',
    'name=~"*EndToEnd*"', 
    'name=~"*e2e*"',
    'name=~"*rcvdPk*"',
    'name=~"*lifetimeSignal*"',
    'name=~"*lifetime*"',
]

for pattern in patterns:
    try:
        df_test = results.get_scalars(
            pattern,
            include_fields=True
        )
        print(f"  {pattern}: {len(df_test)} records")
        if not df_test.empty:
            print(f"    Names: {df_test['name'].unique().tolist()[:5]}")
    except Exception as e:
        print(f"  {pattern}: Error - {e}")

# ============================================================
# 7. Check what's in UdpSink specifically
# ============================================================
print("\n" + "=" * 80)
print("7. ALL SCALARS FROM UDP SINK MODULES")
print("=" * 80)

try:
    # UdpSink is on hosts 4, 9, 14, 19, 24
    df_sink = results.get_scalars(
        'module=~"*.host[4].app[0]"',
        include_fields=True, 
        include_attrs=True, 
        include_runattrs=True, 
        include_itervars=True
    )
    print(f"Records found: {len(df_sink)}")
    if not df_sink.empty:
        print(f"\nUnique 'name' values:")
        for name in sorted(df_sink['name'].unique()):
            print(f"  - {name}")
except Exception as e:
    print(f"Error: {e}")

print("\n" + "=" * 80)
print("DIAGNOSTIC COMPLETE")
print("=" * 80)]]></script>
