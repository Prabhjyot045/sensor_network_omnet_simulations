        <chart createdWith="OMNeT++ 6.3.0.251110-bb3d6a4731" icon="icons/full/obj/scatterchart.png" id="12953409" name="Latency_vs_node_speed" resultTypes="scalar" template="scatterchart_itervars_mpl" type="MATPLOTLIB">
            <script><![CDATA[from omnetpp.scave import results, chart, utils, ideplot
import pandas as pd
import numpy as np
import matplotlib.pyplot as plt

# get chart properties
props = chart.get_properties()
utils.preconfigure_plot(props)

# Query end-to-end delay from sink apps (hosts 4, 9, 14, 19, 24)
try:
    df_delay = results.get_scalars(
        'name=~"endToEndDelay:histogram:mean"',
        include_fields=True, include_attrs=True, include_runattrs=True, include_itervars=True
    )
except results.ResultQueryError as e:
    raise chart.ChartScriptError("Error querying delay data: " + str(e))

if df_delay.empty:
    raise chart.ChartScriptError("The result filter returned no data.")

# Convert speed to integer
df_delay['speed'] = df_delay['speed'].astype(int)

# Extract mobility model from configname
def extract_mobility(configname):
    if 'RandomWaypoint' in configname:
        return 'Random Waypoint'
    elif 'RandomWalk' in configname:
        return 'Random Walk'
    return 'Unknown'

df_delay['mobility'] = df_delay['configname'].apply(extract_mobility)

# Convert delay to milliseconds (value is in seconds)
df_delay['delay_ms'] = df_delay['value'] * 1000

# Calculate mean delay per run first (average across all sink nodes)
delay_per_run = df_delay.groupby(['runID', 'configname', 'mobility', 'speed'])['delay_ms'].mean().reset_index()

# Calculate mean and std for each speed and mobility model (across repetitions)
delay_stats = delay_per_run.groupby(['mobility', 'speed'])['delay_ms'].agg(['mean', 'std']).reset_index()
delay_stats.columns = ['mobility', 'speed', 'delay_mean', 'delay_std']
delay_stats = delay_stats.sort_values(['mobility', 'speed'])

# Plot using current axes
ax = plt.gca()

markers = {'Random Waypoint': 'o', 'Random Walk': 's'}
colors = {'Random Waypoint': 'tab:orange', 'Random Walk': 'tab:blue'}

for mobility in sorted(delay_stats['mobility'].unique()):
    data = delay_stats[delay_stats['mobility'] == mobility].sort_values('speed')
    ax.errorbar(data['speed'], data['delay_mean'], yerr=data['delay_std'], 
                marker=markers.get(mobility, '^'), capsize=5, label=mobility, 
                linewidth=2, markersize=8, color=colors.get(mobility, None))

ax.set_xlabel('Node Speed (m/s)', fontsize=12)
ax.set_ylabel('End-to-End Latency (ms)', fontsize=12)
ax.set_title('End-to-End Latency vs Node Speed', fontsize=14)
ax.legend(loc='best', fontsize=10)
ax.grid(True, linestyle='--', alpha=0.7)
ax.set_xlim(0, 22)
ax.set_xticks([1, 5, 10, 15, 20])

plt.tight_layout()

utils.postconfigure_plot(props)
utils.export_image_if_needed(props)
utils.export_data_if_needed(delay_per_run, props)]]></script>
